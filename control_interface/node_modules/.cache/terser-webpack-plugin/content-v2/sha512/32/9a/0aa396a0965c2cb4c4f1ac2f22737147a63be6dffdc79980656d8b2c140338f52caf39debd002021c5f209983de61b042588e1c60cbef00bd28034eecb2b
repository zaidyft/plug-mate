;\\n      } else {\\n        formattedstart = start;\\n      }\\n      var formattedend = \\\"\\\"\\n      if (end.length === 7) {\\n        formattedend = \\\"0\\\" + end;\\n      } else {\\n        formattedend = end;\\n      }\\n\\n\\n      // Update start time\\n\\n      if (start.length === 7) {\\n        start = \\\"0\\\" + start;\\n      }\\n      if (start.includes(\\\"AM\\\")) {\\n        if (start.substring(0, 2) === \\\"12\\\") {\\n          start = \\\"2017-12-19 00\\\" + start.substring(2, 5) + \\\":00\\\"\\n        } else {\\n          start = \\\"2017-12-19 \\\" + start.substring(0, 5) + \\\":00\\\"\\n        }\\n      } else {\\n        if (start.substring(0, 2) === \\\"12\\\") {\\n          start = \\\"2017-12-19 \\\" + start.substring(0, 5) + \\\":00\\\"\\n        } else {\\n          start = \\\"2017-12-19 \\\" + (Number(start.substring(0, 2)) + 12).toString() + start.substring(2, 5) + \\\":00\\\"\\n        }\\n      }\\n\\n\\n      // Update end time\\n\\n      if (end.length === 7) {\\n        end = \\\"0\\\" + end;\\n      }\\n      if (end.includes(\\\"AM\\\")) {\\n        if (end.substring(0, 2) === \\\"12\\\") {\\n          end = \\\"2017-12-20 00\\\" + end.substring(2, 5) + \\\":00\\\"\\n        } else {\\n          end = \\\"2017-12-19 \\\" + end.substring(0, 5) + \\\":00\\\"\\n        }\\n      } else {\\n        if (end.substring(0, 2) === \\\"12\\\") {\\n          end = \\\"2017-12-19 \\\" + end.substring(0, 5) + \\\":00\\\"\\n        } else {\\n          end = \\\"2017-12-19 \\\" + (Number(end.substring(0, 2)) + 12).toString() + end.substring(2, 5) + \\\":00\\\"\\n        }\\n      }\\n\\n      // Checking conflicts\\n      var hasConflict = false;\\n      var conflictedEvents = [];\\n      var plugload_option = document.querySelectorAll('input[type=\\\"checkbox\\\"]:checked');\\n      var repeat_option = document.querySelector('input[name=\\\"Repeat:\\\"]:checked').value;\\n      var days_to_loop_over = [\\\"2017-12-19\\\"]\\n      if