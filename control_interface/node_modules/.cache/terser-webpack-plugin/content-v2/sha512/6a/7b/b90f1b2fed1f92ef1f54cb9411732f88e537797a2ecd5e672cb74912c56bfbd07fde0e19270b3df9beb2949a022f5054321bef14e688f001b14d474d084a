ct\\\";\\nimport ReactDOM, { render } from 'react-dom'\\nimport moment from 'moment';\\nimport Scheduler, { SchedulerData,ViewTypes } from \\\"react-big-scheduler\\\";\\nimport DemoData from './Data.js';\\nimport withDragDropContext from \\\"./withDnDContext\\\";\\nimport \\\"react-big-scheduler/lib/css/style.css\\\";\\nimport Timepicker from './Timepicker';\\nimport Radio from './Radio';\\nimport Checkbox from './Checkbox';\\nimport './styles.css';\\n\\nclass Basic extends Component {\\n  constructor(props) {\\n    super(props);\\n    let schedulerData = new SchedulerData(\\n      \\\"2017-12-24\\\",\\n      ViewTypes.Day,\\n      false,\\n      false,\\n      {\\n        checkConflict: true,\\n        recurringEventsEnabled: true,\\n        scrollToSpecialMomentEnabled: true, // Allow automatic scroll\\n        crossResourceMove: false // Cannot drag events between plug loads\\n      }, {\\n        getScrollSpecialMomentFunc: this.getScrollSpecialMoment, // Allow automatic scroll\\n        isNonWorkingTimeFunc: this.isNonWorkingTime // Set whole day to \\\"NonWorkingTime\\\", so that grey colour is consistent\\n      }\\n    );\\n    schedulerData.setResources(DemoData.resources); // Set the calendar plug loads to be the plug loads from DemoData\\n    schedulerData.setEvents(DemoData.events); // Set the calendar events to be the events from DemoData\\n    this.state = {\\n      viewModel: schedulerData\\n    };\\n    window.sundayCalendar = this\\n  }\\n\\n  render() {\\n    const { viewModel } = this.state;\\n    return (\\n      <div>\\n        <div>\\n          <Scheduler\\n            schedulerData={viewModel}\\n            eventItemClick={this.eventClicked}\\n            updateEventStart={this.updateEventStart}\\n            updateEventEnd={this.updateEventEnd}\\n            moveEvent={this.moveEvent}\\n            newEvent={this.newEvent}\\n            conflictOccurred={this.conflictOccurred}\\n          />\\n        </div>\\n      </div>\\n    );\\n  }\\n\\n  // Set the whole day as \\\"NonWorkingTime\\\", so that grey colour is consistent\\n  isNonWorkingTime = (schedulerData, time) => {\\n    return true;\\n  }\\n\\n  // Automatic scroll to 7 AM\\n  getScrollSpecialMoment = (schedulerData, startMoment, endMoment) => {\\n    return moment(\\\"2017-12-24 07:00:00\\\");\\n  }\\n\\n  // Function for when event is clicked\\n  eventClicked = (schedulerData, event) => {\\n    // Function for when the OK button on the popup is clicked\\n    function okButtonClicked() {\\n      // Format schedule name\\n      var schedule = document.getElementById(\\\"schedule_name\\\").value;\\n      var start = document.getElementById(\\\"start\\\").value;\\n      var end = document.getElementById(\\\"end\\\").value;\\n      var formattedstart = \\\"\\\";\\n      if (start.length === 7) { formattedstart = \\\"0\\\" + start; } else { formattedstart = start; }\\n      var formattedend = \\\"\\\";\\n      if (end.length === 7) { formattedend = \\\"0\\\" + end; } else { formattedend = end; }\\n\\n      // Format start time\\n      if (start.length === 7) { start = \\\"0\\\" + start; } // For example, 9:00 AM is changed to 09:00 AM\\n      if (start.includes(\\\"AM\\\")) {\\n        if (start.substring(0, 2) === \\\"12\\\") {\\n          start = \\\"2017-12-24 00\\\" + start.substring(2, 5) + \\\":00\\\";\\n        } else {\\n          start = \\\"2017-12-24 \\\" + start.substring(0, 5) + \\\":00\\\";\\n        }\\n      } else {\\n        if (start.substring(0, 2) === \\\"12\\\") {\\n          start = \\\"2017-12-24 \\\" + start.substring(0, 5) + \\\":00\\\";\\n        } else {\\n          start = \\\"2017-12-24 \\\" + (Number(start.substring(0, 2)) + 12).toString() + start.substring(2, 5) + \\\":00\\\";\\n        }\\n      }\\n\\n      // Fomat end time\\n      if (end.length === 7) { end = \\\"0\\\" + end; } // For example, 9:00 AM is changed to 09:00 AM\\n      if (end.includes(\\\"AM\\\")) {\\n        if (end.substring(0, 2) === \\\"12\\\") {\\n          end = \\\"2017-12-24 00\\\" + end.substring(2, 5) + \\\":00\\\";\\n        } else {\\n          end = \\\"2017-12-24 \\\" + end.substring(0, 5) + \\\":00\\\";\\n        }\\n      } else {\\n        if (end.substring(0, 2) === \\\"12\\\") {\\n          end = \\\"2017-12-24 \\\" + end.substring(0, 5) + \\\":00\\\";\\n        } else {\\n          end = \\\"2017-12-24 \\\" + (Number(end.substring(0, 2)) + 12).toString() + end.substring(2, 5) + \\\":00\\\";\\n        }\\n      }\\n\\n      // Checking conflicts\\n      var hasConflict = false;\\n      var plugload_option = document.querySelectorAll('input[type=\\\"checkbox\\\"]:checked');\\n      var repeat_option = document.querySelector('input[name=\\\"Repeat:\\\"]:checked').value;\\n      var days_to_loop_over = [\\\"2017-12-24\\\"];\\n      if (repeat_option === \\\"Every Day\\\") {\\n        days_to_loop_over = ['2017-12-18', '2017-12-19', '2017-12-20', '2017-12-21', '2017-12-22', '2017-12-23', '2017-12-24'];\\n      }\\n      schedulerData.events.forEach(function(e) {\\n        var eStart = e.start;\\n        var eEnd = e.end;\\n        var start_for_this = \\\"\\\";\\n        var end_for_this = \\\"\\\";\\n        for (var day of days_to_loop_over) {\\n          // Check for the repeat option chosen\\n          if (e.resourceId === event.resourceId) {\\n            // Check for the current selected plug load\\n            start_for_this = day + start.substring(10, 19);\\n            end_for_this = day + end.substring(10, 19);\\n            if (((start_for_this >= eStart && start_for_this < eEnd) ||\\n                (end_for_this > eStart && end_for_this <= eEnd) ||\\n                (eStart >= start_for_this && eStart < end_for_this) ||\\n                (eEnd > start_for_this && eEnd <= end_for_this)) && (e.id !== event.id)) {\\n                  hasConflict = true;\\n                }\\n          }\\n          for (var plugload of plugload_option) {\\n            // Check for other plug loads selected under \\\"Also apply to:s\\\"\\n            if (e.resourceId === plugload.value) {\\n              start_for_this = day + start.substring(10, 19);\\n              end_for_this = day + end.substring(10, 19);\\n              if ((start_for_this >= eStart && start_for_this < eEnd) ||\\n                  (end_for_this > eStart && end_for_this <= eEnd) ||\\n                  (eStart >= start_for_this && eStart < end_for_this) ||\\n                  (eEnd > start_for_this && eEnd <= end_for_this)) {\\n                    hasConflict = true;\\n                  }\\n            }\\n          }\\n        }\\n      })\\n\\n      // If there is a conflict, show alert and do nothing. Otherwise, continue to update the calendar.\\n      if (hasConflict) {\\n        window.sundayCalendar.conflictOccurred();\\n      } else {\\n        // Update event name\\n        event.title = schedule + \\\" (\\\" + formattedstart + \\\" - \\\" + formattedend + \\\")\\\";\\n\\n        // Update event start and end times\\n        schedulerData.updateEventStart(event, start);\\n        schedulerData.updateEventEnd(event, end);\\n\\n        // Update the repeat option\\n        var rrule = \\\"\\\"\\n        if (repeat_option === \\\"Every Day\\\") {\\n          var updatedstart = \\\"2017-12-18\\\" + start.substring(10, 19)\\n          schedulerData.updateEventStart(event, updatedstart)\\n          var updatedend = \\\"2017-12-18\\\" + end.substring(10, 19)\\n          schedulerData.updateEventEnd(event, updatedend)\\n          event.rrule=\\\"FREQ=DAILY;DTSTART=20171218T000000Z;UNTIL=20171224T235900Z\\\"\\n          rrule=\\\"FREQ=DAILY;DTSTART=20171218T000000Z;UNTIL=20171224T235900Z\\\"\\n          setTimeout(function() {document.getElementById(\\\"SundayCalendar\\\").click();},1)\\n        } else {\\n          var name = event.title;\\n          var new_start = event.start\\n          var new_end = event.end\\n          var new_slot = event.resourceId\\n          schedulerData.removeEve